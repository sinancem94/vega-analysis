    "@typescript-eslint/parser": "^4.33.0",
    "bootstrap": "^5.2.3",
    "electron": "^22.2.0",
    "eslint": "^7.32.0",
    "exceljs": "^4.3.0",
    "jquery": "^3.6.4",
    "typescript": "^4.8.3"
  },
  "dependencies": {
